@page
@model WXWork.Areas.Admin.Pages.Suite.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TSuite[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TSuite[0].Secret)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TSuite[0].Ticket)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TSuite[0].DTTicket)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TSuite[0].Token)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TSuite[0].DTTokenExpires)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.TSuite) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Secret)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ticket)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DTTicket)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Token)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DTTokenExpires)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
